{
  "name": "tykon",
  "description": "Generate Kotlin Source Files from TypeScript Definitions",
  "type": "module",
  "version": "1.0.0",
  "author": "gmitch215",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test-log": "jest --logHeapUsage --detectOpenHandles > jest.log",
    "doc": "bunx typedoc src/index.ts --entryPointStrategy Expand src --out docs --plugin typedoc-github-theme --excludePrivate --excludeProtected"
  },
  "bin": "dist/index.js",
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@cloudflare/workers-types": "^4.20250529.0",
    "@jest/globals": "^29.7.0",
    "@types/bun": "latest",
    "@types/jest": "^29.5.14",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "typedoc-github-theme": "^0.3.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "ts-morph": "^26.0.0",
    "tykon": "file:"
  }
}
